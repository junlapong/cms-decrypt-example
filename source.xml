<!-- LSS Initiate Request XML -->
<LSSecure>
	<InitRequest>
		<Version></Version>
		<TxnID></TxnID>
		<MerchantDetails>
			<AcquirerBIN></AcquirerBIN>
			<MID></MID>
			<Name></Name>
		</MerchantDetails>
		<PurchaseDetails>
			<CardNumber></CardNumber>
		</PurchaseDetails>
	</InitRequest>
</LSSecure>

<!-- LSS Initiate Response XML -->
<LSSecure>
	<InitResponse>
		<Version></Version>
		<TxnID></TxnID>
		<Status></Status>
		<ITMXToken></ITMXToken>
		<AuthURL></AuthURL>
		<RespCode></RespCode>
		<RespDescription></RespDescription>
	</InitResponse>
</LSSecure>

